    "react-scrollbar-js": "^1.0.1",
    "react-select": "^1.0.0-rc.5",
    "react-select-plus": "^1.0.0-rc.5",
    "react-select2-wrapper": "^1.0.4-beta5",
    "react-table": "^6.5.3",
    "recharts": "^1.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-logger": "latest",
    "redux-promise": "0.5.3",
    "redux-saga": "^0.15.6",
